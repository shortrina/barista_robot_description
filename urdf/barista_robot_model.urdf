        <parent link="front_roll_link"/>
        <child link="front_pitch_link"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000.0" velocity="100.0"/>
        <dynamics damping="0.0" friction="0.1"/>
    </joint>

    <!-- Caster Wheel Back -->
    <link name="back_yaw_link">
        <visual>
            <origin rpy="0 1.5707 1.5707" xyz="0.0 0 0.0"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
            <material name="black"/>
        </visual>

        <collision>
            <origin rpy="0 1.5707 1.5707" xyz="0.0 0 0.0"/>
            <geometry>
                <cylinder length="0.001" radius="0.0045000000000000005"/>
            </geometry>
        </collision>

        <inertial>
            <origin rpy="0 1.5707 1.5707" xyz="0.0 0 0.0"/>
            <mass value="0.001"/>
            <inertia ixx="5.145833333333334e-09" ixy="0" ixz="0" iyy="5.145833333333334e-09" iyz="0"
                     izz="1.0125000000000003e-08"/>
        </inertial>

    </link>

    <joint name="back_yaw_joint" type="continuous">
        <origin rpy="0 0 0" xyz="-0.13 0 0.0045"/>
        <parent link="main_body_link"/>
        <child link="back_yaw_link"/>
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" velocity="100.0"/>
        <dynamics damping="0.0" friction="0.1"/>
