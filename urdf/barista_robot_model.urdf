            <geometry>
                <sphere radius="0.0277"/>
            </geometry>
            <material name="black"/>
        </visual>

        <collision>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.0277"/>
            </geometry>
        </collision>

        <inertial>
            <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
            <mass value="0.06"/>
            <inertia ixx="1.8414959999999996e-05" ixy="0.0" ixz="0.0" iyy="1.8414959999999996e-05" iyz="0.0"
                     izz="1.8414959999999996e-05"/>
        </inertial>
    </link>

    <gazebo reference="back_pitch_link">
        <kp>1000000000000000000000000000.0</kp>
        <kd>1000000000000000000000000000.0</kd>
        <mu1>0.5</mu1>
        <mu2>0.5</mu2>
        <material>Gazebo/Black</material>
    </gazebo>

